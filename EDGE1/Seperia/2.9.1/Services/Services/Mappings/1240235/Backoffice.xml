<?xml version="1.0" encoding="utf-8" ?>	
						<Mappings>
							<Object Type="GenericMetricsUnit">
								<Map To='SegmentDimensions[{GetSegment("Tracker")}]' >
									<Read Var="tracker" Field='ID' Required='true'/>
									<Map To='Value' Value='{tracker}'/>
								</Map>
								<Map To='MeasureValues[{GetMeasure("ClientSpecific1")}]' Field='TotalLeads' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific2")}]' Field='Self-Activation' Var='Sat'/>
								<Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Field='DepositTotal' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific4")}]' Field='Redeposit' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific5")}]' Field='NoAnswer' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific6")}]' Field='DepositLeadsCountTotal' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific7")}]' Field='PaymentDeclined' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific8")}]' Field='RedepositLeadsCount' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific9")}]' Field='FirstDepositLeadsCount' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific10")}]' Field='FirstDeposit' />
							</Object>
						</Mappings>
