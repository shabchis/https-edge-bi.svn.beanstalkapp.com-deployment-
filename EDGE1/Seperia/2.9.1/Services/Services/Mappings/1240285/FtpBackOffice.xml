<?xml version="1.0" encoding="utf-8" ?>         
<Mappings>
<Object Type="GenericMetricsUnit">
<Map To='SegmentDimensions[{GetSegment("Tracker")}]' >
<Read Var="tracker" Field='Tracker' />
<Map To='Value' Value="{UrlEncode(tracker)}"/>
</Map>


<Read Var="dayCode" Field="Date" Regex="(?{d}[0-9]{2})/(?{m}[0-9]{2})/(?{y}[0-9]{4})"/>
<Map To="TimePeriodStart" Value='{CreatePeriodStart(dayCode.y, dayCode.m, dayCode.d)}'/>
<Map To="TimePeriodEnd" Value="{CreatePeriodEnd(dayCode.y, dayCode.m, dayCode.d)}"/>                                                                        
<Map To="Account" Value="{GetAccount(AccountName)}" Field="AccountName"/>
<Map To="Channel">
<Map To="ID" Value="-1"/>
</Map>

<Map To='MeasureValues[{GetMeasure("ClientSpecific1")}]' Var='OptionBitRegs' Field= '24Options - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific2")}]' Var='OptionBitDepositors' Field= '24Options - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Var='OptionRegs' Field= '24Option - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific4")}]' Var='OptionDepositors' Field= 'BBinary - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific5")}]' Var='OptionClicks' Field= 'BBinary - Depositors' /> 
<Map To='MeasureValues[{GetMeasure("ClientSpecific6")}]' Var='PowerDepositors' Field= 'BBinary - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific7")}]' Var='BBinaryRegs' Field= 'OptionWeb - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific8")}]' Var='BBinaryDepositors' Field= 'OptionWeb - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific9")}]' Var='BBinaryClicks' Field= 'OptionWeb - Clicks' /> 
<Map To='MeasureValues[{GetMeasure("ClientSpecific10")}]' Var='InteractiveDepositors' Field= 'Anyoption - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific11")}]' Var='TradeRushRegs' Field= 'AnyOption - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific12")}]' Var='TradeRushDepositors' Field= 'AnyOption - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific13")}]' Var='TradeRushClicks' Field= 'BinaryTilt - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific14")}]' Var='BDSwissDepositors' Field= 'BinaryTilt - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific15")}]' Var='StockPairRegs' Field= 'BinaryTilt - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific16")}]' Var='StockPairDepositors' Field=  'Opteck - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific17")}]' Var='StockPairClicks' Field= 'Opteck - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific18")}]' Var='PowerRevenue' Field= Opteck - Clicks
<Map To='MeasureValues[{GetMeasure("ClientSpecific19")}]' Var='OptionWebRegs' Field= 'OptionWeb - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific20")}]' Var='OptionWebDepositors' Field= 'OptionTime - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific21")}]' Var='OptionWebClicks' Field= 'OptionTime - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific22")}]' Var='OptionWebClicks' Field= 'PowerOption - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific23")}]' Var='CedarRegs' Field= 'PowerOption - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific24")}]' Var='CedarDepositors' Field= 'PowerOption - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific25")}]' Var='CedarClicks' Field= 'RB Options - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific26")}]' Var='OpteckRegs' Field= 'RB Options - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific27")}]' Var='OpteckDepositors' Field= 'TopOption - Regs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific28")}]' Var='OpteckClicks' Field= 'TopOption - Depositors' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific29")}]' Var='AnyOptionDepositors' Field= 'TopOption - Clicks' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific30")}]' Var='AnyOptionClicks' Field= 'TopOption - Revenue' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific31")}]' Var='AnyOptionRegs' Field= 'RB Options - Clicks' />


</Object>
<Object Type="Edge.Data.Objects.Signature">
<Read Var="dayCode" Field="Date" Regex="(?{d}[0-9]{2})/(?{m}[0-9]{2})/(?{y}[0-9]{4})"/>
<Read Var="AccountName" Field="AccountName"/>
<Map To='Value' Value='{dayCode.y}{dayCode.m}{dayCode.d}_{AccountName}}' />
</Object>
</Mappings>
