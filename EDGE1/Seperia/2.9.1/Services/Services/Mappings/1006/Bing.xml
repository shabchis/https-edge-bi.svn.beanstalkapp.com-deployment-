<?xml version="1.0" encoding="utf-8" ?>	
						<Mappings>
                                  <Using Namespace="Edge.Data.Objects.{0}, Edge.Data.Pipeline"/>
                                  <Object Type="Ad">
										 <Read Var="destUrl2" Field="DestinationUrl" Regex='(\bp=edgetrackerid(?{edgetrackerid}\w+)\b)' Required="false"/>
										 <Read Var="destUrl1" Field='DestinationUrl' Regex='(\bp=edgetrackerid_(?{edgetrackerid}\w+)\b)' Required="false"/>									
										 <Map To='Segments[{GetSegment("Tracker")}]::SegmentObject' Condition='destUrl1 != null || destUrl2 != null'>
                                                <Map To='Value'  Condition='destUrl1 != null' Value="{destUrl1.edgetrackerid}" />
												<Map To='Value'  Condition='destUrl2 != null' Value="{destUrl2.edgetrackerid}" />
                                                <Map To='OriginalID'  Condition='destUrl1 != null' Value="{destUrl1.edgetrackerid}"/>
												<Map To='OriginalID'  Condition='destUrl2 != null' Value="{destUrl2.edgetrackerid}"/>
                                         </Map>
                                  </Object>
