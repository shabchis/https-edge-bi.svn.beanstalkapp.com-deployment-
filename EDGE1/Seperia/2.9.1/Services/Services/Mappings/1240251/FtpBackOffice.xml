<?xml version="1.0" encoding="utf-8" ?>	
						<Mappings>
							<Object Type="GenericMetricsUnit">
								<Map To='SegmentDimensions[{GetSegment("Tracker")}]' >
									<Read Var="tracker" Field='Gateway_id' />
									<Map To='Value' Value='{tracker}'/>
								</Map>		
								
								<Read Var="dayCode" Field="Day_code" Regex="(?{y}[0-9]{4})(?{m}[0-9]{2})(?{d}[0-9]{2})"/>
								<Map To="TimePeriodStart" Value='{CreatePeriodStart(dayCode.y, dayCode.m, dayCode.d)}'/>
								<Map To="TimePeriodEnd" Value="{CreatePeriodEnd(dayCode.y, dayCode.m, dayCode.d)}"/>						
								<Map To="Account" Value="{GetAccount(AccountName)}" Field="AccountName"/>
								<Map To="Channel" >
									<Map To="ID" Value="-1"/>
								</Map>
																
								<Map To='MeasureValues[{GetMeasure("ClientSpecific1")}]' Field= 'Signup' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific2")}]' Field='Premium' />
								<!-- <Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Field='CreateApp' />	
								<Map To='MeasureValues[{GetMeasure("ClientSpecific4")}]' Field= 'GenerateApp' /> -->
								<Map To='MeasureValues[{GetMeasure("ClientSpecific5")}]' Field='Silver' />								
								<Map To='MeasureValues[{GetMeasure("ClientSpecific6")}]' Field='Gold' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific7")}]' Field='Platinum' />	
								<Map To='MeasureValues[{GetMeasure("ClientSpecific8")}]' Field= 'Diamond' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific9")}]' Field='Dynamic' />								
							</Object>
							<Object Type="Edge.Data.Objects.Signature">
								<Read Var="dayCode" Field="Day_code" Regex="(?{y}[0-9]{4})(?{m}[0-9]{2})(?{d}[0-9]{2})"/>
								<Read Var="AccountName" Field="AccountName"/>
								<Map To='Value' Value='{dayCode.y}{dayCode.m}{dayCode.d}_{AccountName}}' />
							</Object>
						</Mappings>
