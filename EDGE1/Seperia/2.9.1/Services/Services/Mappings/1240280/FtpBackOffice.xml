<?xml version="1.0" encoding="utf-8" ?>	
<Mappings>
<Object Type="GenericMetricsUnit">
<Map To='SegmentDimensions[{GetSegment("Tracker")}]' >
<Read Var="tracker" Field='Serial' />
<Map To='Value' Value='{tracker}'/>
</Map>		

<Read Var="dayCode" Field="Date" Regex="(?{y}[0-9]{4})(?{m}[0-9]{2})(?{d}[0-9]{2})"/>
<Map To="TimePeriodStart" Value='{CreatePeriodStart(dayCode.y, dayCode.m, dayCode.d)}'/>
<Map To="TimePeriodEnd" Value="{CreatePeriodEnd(dayCode.y, dayCode.m, dayCode.d)}"/>						
<Map To="Account" Value="{GetAccount(AccountName)}" Field="AccountName"/>
<Map To="Channel" >
<Map To="ID" Value="-1"/>
</Map>

<Map To='MeasureValues[{GetMeasure("ClientSpecific1")}]' Var='NumOfSDMDLClicked' Field= 'Num_of_SDM_DL_Clicked' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific2")}]' Var='NumofInstalls' Field= 'Num_of_Installs' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Var='NumOfNPU' Field= 'Num_of_NPU' />
<Map To='MeasureValues[{GetMeasure("ClientSpecific4")}]' Var='PredictedActiveDays' Field= 'Predicted_Active_Days' />
								

</Object>
<Object Type="Edge.Data.Objects.Signature">
<Read Var="dayCode" Field="Date" Regex="(?{y}[0-9]{4})(?{m}[0-9]{2})(?{d}[0-9]{2})"/>
<Read Var="AccountName" Field="AccountName"/>
<Map To='Value' Value='{dayCode.y}{dayCode.m}{dayCode.d}_{AccountName}}' />
</Object>
</Mappings>
