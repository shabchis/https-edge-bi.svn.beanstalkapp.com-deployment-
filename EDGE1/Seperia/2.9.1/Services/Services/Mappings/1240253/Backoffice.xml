<?xml version="1.0" encoding="utf-8" ?>	
						<Mappings>
							<Object Type="GenericMetricsUnit">
								<Map To='SegmentDimensions[{GetSegment("Tracker")}]' >
									<Read Var="tracker" Field='Serial' />
									<Map To='Value' Value='{tracker}'/>
								</Map>
								<Map To='MeasureValues[{GetMeasure("ClientSpecific1")}]' Field='Registrations' />
								<Map To='MeasureValues[{GetMeasure("ClientSpecific2")}]' Field='DepositCount'/>
								<Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Field='FirstDepositAmount' />								
							</Object>
						</Mappings>
