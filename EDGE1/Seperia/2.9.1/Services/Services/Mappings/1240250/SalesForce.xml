<?xml version="1.0" encoding="utf-8" ?>						

<Mappings>
<Object Type="GenericMetricsUnit">

<Map To='SegmentDimensions[{GetSegment("Tracker")}]'>
<Read Var="tracker" Field='Edge_Tracker_New__c' Required='true'/>
<Map To='Value' Value='{tracker}' Condition='(tracker != null)' Required='false'/>
</Map>

<!--Getting Leads-->
<Read Var="Status" Field='Status' Required='true'/> 
<Read Var="CreatedDate" Field='CreatedDate' Required='true'/>
<Read Var="MQL" Field='MQL__c' Type='boolean' Required='true'/>


 <!--Total Leads-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Condition='CreatedDate != null &amp;&amp; Status!=null' Value='1' /> <!--Total Leads-->

<!--Leads-->

<Map To='MeasureValues[{GetMeasure("ClientSpecific4")}]' Condition='MQL = true' Value='1'/><!--MQL-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific6")}]' Condition='Status == "Qualified"' Value='1'/><!--SQL-->


<!--Opportunities-->

<Read Var="Revenue" Field='Amount' Required='false'/>
<Read Var="Stage" Field='ConvertedOpportunity.StageName' Required='false'/>

<Map To='MeasureValues[{GetMeasure("ClientSpecific8")}]' Condition='Revenue != null'  Value='{Revenue}'/><!--Revenue Amount-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific9")}]' Condition='Stage == "Closed Won"' Value='1' /><!--Closed Won Opportunity-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific5")}]' Condition='Stage != null' Value='1' /><!--Total Opportunities-->

</Object>
</Mappings>
