<?xml version="1.0" encoding="utf-8" ?>						

<Mappings>
<Object Type="GenericMetricsUnit">

<Map To='SegmentDimensions[{GetSegment("Tracker")}]'>
<Read Var="tracker" Field='Edge_Tracker__c' Required='false'/>
<Map To='Value' Value='{tracker}' Condition='(tracker != null)' Required='false'/>
</Map>

<!--Getting Leads-->
<Read Var="Status" Field='Status' Required='false'/> 
<Read Var="CreatedDate" Field='CreatedDate' Required='false'/>
<Read Var="MQL" Field='MQL__c' Required='false'/>
<Read Var="Isconv" Field='Isconverted' Required='false'/>
<Read Var="SQL" Field='Lead_Stage__c' Required='false'/>

 <!--Total Leads-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific3")}]' Condition='CreatedDate != null &amp;&amp; Status!=null' Value='1' /> <!--Total Leads-->

<!--Leads-->

<Map To='MeasureValues[{GetMeasure("ClientSpecific4")}]' Condition='MQL == "true"' Value='1'/>
<Map To='MeasureValues[{GetMeasure("ClientSpecific6")}]' Condition='SQL == "true"' Value='1'/>


<!--Opportunities-->

<Read Var="Revenue" Field='Amount' Required='false'/>

<Map To='MeasureValues[{GetMeasure("ClientSpecific8")}]' Condition='Revenue != null'  Value='{Revenue}'/><!--Revenue Amount-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific9")}]' Condition='StageName == "Closed Won"' Value='1' /><!--Closed Won Opportunity-->
<Map To='MeasureValues[{GetMeasure("ClientSpecific5")}]' Condition='StageName != null' Value='1' /><!--Total Opportunities-->

</Object>
</Mappings>
